@charset "utf-8";
/*reset*/

html, body, div, span, object, h1, h2, h3, h4, h5, h6, a, em, ins, small, strong, b, i, dl, dt, dd, form, label, table, tbody, tfoot, thead, tr, th, td, p, ul, li {  margin: 0;  padding: 0;  border: 0;  outline: 0;  font-size: 100%;  vertical-align: baseline;  background: transparent;}
img {  border: 0;  outline: 0;  font-size: 100%;  background: transparent;}
body {  line-height: 1.3;}
ul {  list-style: none;}
table {  border-collapse: collapse;  border-spacing: 0;}
html, body {height:100%;}

html {background: url(<%= image_path 'store/bg3.png' %>) no-repeat left top;}

body {background: url(<%= image_path 'store/bg2.png' %>) no-repeat left top; font-family: arial, tahoma, sans-serif; font-size:100.01%;color: #50504b;} 

a {color:#494949;}

a:hover {text-decoration:none;}

.wrapper {
	min-height:100%;
	height:auto  !important;
	height:100%;position:relative;
	font-size:12px;
	margin: 0 auto;
	color: #666666;
	min-width: 1260px;}

.wrapper_top{
	background: url(<%= image_path 'store/bg.png' %>) no-repeat top right;
	font-size:12px;
}

.wrapper_bottom{
	font-size:12px;
}

.head {border-top: 5px solid #65c200; height: 185px; position:relative; padding-left: 340px; padding-top: 36px;}
.logo {position:absolute; z-index: 6;left:2px; top:0px; width: 227px; height: 71px;}
.logo a {display: block; width: 227px; height: 71px; background: url(<%= image_path 'store/logo.png' %>) no-repeat top center; position: absolute; top: 16px; left: 24px; z-index: 5;}
.logo a:hover {background: url(<%= image_path 'store/logoon.png' %>) no-repeat top center;}

.er {
  position: absolute;
  top: -5px;
  right: 45px;
  background: #65c200;
  color: #fff;
  padding: 5px 10px;
  text-transform: uppercase;
  font-size: 11px;
  -moz-border-radius: 6px; -webkit-border-radius: 6px; -khtml-border-radius: 6px; border-radius: 6px; 
}

.er a{
  color: #fff;
}

#menu {background: url(<%= image_path 'store/menubg.gif' %>) repeat-x top left;height: 46px;margin-left: 20px;margin-right: 66px;position: relative;}
#menu .last {  position: absolute;  right: -23px;  width: 23px;  height: 46px;  background: url(<%= image_path 'store/menubg2.png' %>) no-repeat top right;}
#menu ul {  background: url(<%= image_path 'store/menu.png' %>) no-repeat top left;  height: 46px;  padding-left: 30px;  margin-left: -20px;}
#menu ul li{  float: left;}
#menu ul li a{  color: #fff;  font-size: 15px; line-height: 46px;  padding: 0 12px;}

.slb {  width: auto;  overflow: hidden;  padding-right: 40px;  padding-top: 4px;}

.phone {
  background: url(<%= image_path 'store/phone.png' %>) no-repeat 15px 20px;
  float: left;
  width: 33%;
  min-height: 100px;
}

.phone div {
  padding-left: 100px;
  font-weight: normal;  color: #666666;  
  font-size: 18px;
  padding-top: 13px;
  margin-bottom: 5px;
}

.phone span.numb {
  line-height: 23px;
  display: block;
  color: #ff5200;
  font-weight: bold;
  font-size: 23px;
}

.phone .back-call {
  font-size: 10px;
  display: block;
  text-transform: uppercase;
}

.search {  width: 43%; position: relative;  background: #fcefc5 url(<%= image_path 'store/search.png' %>) no-repeat 16px 14px;  margin-bottom: 20px; -moz-border-radius: 8px; -webkit-border-radius: 8px; -khtml-border-radius: 8px; border-radius: 8px;  border: 1px solid red;  padding: 36px 0 28px 0;  float: left;  }
.search h2 {  position: absolute;  top: 15px;  left: 100px;  font-weight: normal;  color: #666666;  font-size: 17px;}
.search-product span.leftpart {  position: absolute;  display: block;  width: 51px;  height: 56px;  background: url(<%= image_path 'store/slp.png' %>) no-repeat;  left: -50px;  top: -19px;}
.search-product span.rightpart {  position: absolute;  display: block;  width: 9px;  height: 38px;  background: url(<%= image_path 'store/srp.png' %>) no-repeat;  right: -6px;  top: -1px;}
.search-product {  padding: 0 20px 0 106px;}
.search-product div {  position: relative;  background: #fff;  height: 36px;  border: 1px solid #afafaf;}
.search-product input {  position: absolute;  margin-left: -10px;    background: transparent;  width: 100%;  line-height: 30px;  height: 100%;  padding: 0;  border-width: 0px;  font-size: 20px;}
.search-product input:focus {  outline: none;}

.find {  background: url(<%= image_path 'store/arrowb.png' %>) no-repeat center right;  padding-right: 15px;  position: absolute;  bottom: 10px;  right: 16px;}

.card {  background: url(<%= image_path 'store/card.png' %>) no-repeat right 10px;  float: right;  width: 23%;  min-width: 197px;  max-width: 220px;  min-height: 100px;  color: #222222;}
.card h2 {  font-weight: normal;  color: #666666;  font-size: 17px;  padding-top: 15px;  margin-bottom: 5px;}

h1 {  font-size: 35px;  font-weight: normal;  margin: 0 0 20px 0;  padding: 0 0 0 24px;  color: #1d1d1b;}

.left-column {  float: left;  width: 340px;  padding-top: 0px;  font-size: 13px;}
.left-column .left-menu li {  border-bottom: 1px dashed #d9d9d9;  padding: 4px 0;  position: relative;  padding-right: 25px;}
.left-column .left-menu {  margin-left: 35px;  width: 240px;}
.left-column .left-menu  a {  text-decoration: none;}
.left-column .left-menu  a span {  text-decoration: underline;}
.left-column .left-menu  a:hover span {  text-decoration: none;}
.left-column .left-menu  a span.number { color:#55ec0f;  position: absolute;  right: 0;  text-align: right;  padding-right: 8px;  font-size: 10px;  top: 7px;  background: url(img/arrow.png) no-repeat right;  text-decoration: none;}
.left-column .left-menu  ul {  margin: 0; width: 240px;}
.left-column .left-menu  ul li {  border-width: 0;  padding: 2px 7px;  font-size: 90%;  padding-right: 25px;}
.left-column .left-menu  ul li a {  color: #666666;}

.news {  padding: 36px 0 0 36px;  color: #1d1d1b;  width: 240px;}
.news h2 {font-weight: normal; font-size: 17px; margin-bottom: 12px;}
.news .date {  display: block; font-weight: bold; color: #afafaf; font-size: 15px; padding-left: 10px; background: url(img/arrowg.png) no-repeat center left;}
.news p {  margin-bottom: 10px; font-size: 12px;}
.news .all-news { border-bottom: 1px dashed #d9d9d9;  position: relative; margin-top: 25px; width: 140px;}
.news .all-news a {  position: absolute;  border: 1px solid #ff5200;  color: #ff5200;  display: block;  width: 86px;  height: 23px;  text-align: center;  -moz-border-radius: 4px; -webkit-border-radius: 4px; -khtml-border-radius: 4px; border-radius: 4px;  background: #fff;  line-height: 23px;  top: -12px;  right: -100px;}

.subscription {
  width: 242px;
  height: 109px;
  position: relative;
  background: url(img/subscr.png) no-repeat top left;
  margin: 40px 0 0 36px;
  color: #fff;
}

.subscription b {
  display: block;
}
.subscription h2 {
  position: absolute;
  font-size: 18px;
  line-height: 20px;
  font-weight: normal;
  top: 10px;
  left: 24px;
}

.subscription input {
  position: absolute;
  -moz-border-radius: 4px; -webkit-border-radius: 4px; -khtml-border-radius: 4px; border-radius: 4px;
  border-width: 0;
  position: absolute;
  top: 58px;
  height: 24px;
  width: 198px;
  left: 24px;
}

.subscription a {
  color: #fff;
  position: absolute;
  top: 87px;
  left: 24px;
  line-height: 13px;
  background: url(img/arroww.png) no-repeat right 4px;
  padding-right: 13px;
}

.clear {  clear: both;}

.content {margin-left: 348px; padding-right: 46px;}

.action {  text-align: center;  position: relative;  margin-bottom: 40px;}
        
.action .lpic{  display: block;  border: 2px solid #ececec;  margin-bottom: 20px; -moz-border-radius: 6px; -webkit-border-radius: 6px; -khtml-border-radius: 6px; border-radius: 6px;  display: block;  background: #ffffff;  padding: 5px 0;}
.action .lpic:hover {  border: 2px solid #65c200;}
.icn {  display: inline-block;  position: relative;}
.icn a {  margin: 0 3px;}         
.arrow-left, .arrow-right {  position: absolute;  top: 1px;  width: 18px;  height: 37px;}
.arrow-left {background: url(img/arrowl.png) no-repeat top left;  left: -30px;}
.arrow-left:hover {background: url(img/arrowlon.png) no-repeat top left;}
.arrow-right {background: url(img/arrowr.png) no-repeat top left;  right: -30px;}
.arrow-right:hover {background: url(img/arrowron.png) no-repeat top left;}
.icn a img {  border: 2px solid #8b8b8b;}
.icn a:hover img {  border: 2px solid #65c200;}

.cloud {  width: auto;overflow: hidden;text-align: center; margin-bottom: 60px; }
.cloud li {  display: inline;}
.cloud .w1 {  font-size: 100%;  line-height: 100%;}
.cloud .w2 {  font-size: 120%;  line-height: 100%;}
.cloud .w3 {  font-size: 140%;  line-height: 100%;}
.cloud .w4 {  font-size: 160%;  line-height: 100%;}
.cloud .w5 {  font-size: 180%;  line-height: 100%;}
.cloud .w6 {  font-size: 200%;  line-height: 100%;}
.cloud .w7 {  font-size: 220%;  line-height: 100%;}
.cloud .w8 {  font-size: 240%;  line-height: 100%;}
.cloud .w9 {  font-size: 250%;  line-height: 100%;}


.popular { -moz-border-radius: 6px; -webkit-border-radius: 6px; -khtml-border-radius: 6px; border-radius: 6px; background: #E6EFC5; padding-top: 24px; padding-bottom: 24px;}
.goods {padding-left: 28px; padding-right: 28px; overflow:hidden; width:auto; margin-bottom: 40px;}

.item {  float: left;  width: 136px;  margin-right: 18px;}
.item a {  color: #1d1d1b;  font-size: 11px;  text-decoration: none;}
.pic {  background: #fff;  border: 2px solid #ececec;  display: block;  text-align: center;  line-height: 0;}
.popular .pic {  border-width: 0px;}
.item p {  text-decoration: underline;}
.item .price {  color: #65c200;  font-size: 22px;  font-weight: bold;}

.notice {background: #fcefc5; padding: 24px; -moz-border-radius: 6px; -webkit-border-radius: 6px; -khtml-border-radius: 6px; border-radius: 6px; min-height: 10px; color: #9e3300; font-size: 15px;}

.footer {
  clear: both;
  width: auto;
  overflow: hidden;  
  padding: 36px 0;
}

.footer .left-footer {
  float: left;
  width: 240px; 
  padding-left: 46px;
}

.footer .left-footer .blogo {
  display: block;
  width: 145px;
  height: 44px;
  background: url(<%= image_path 'store/logo1.png' %>) no-repeat right 4px;
}

.footer .left-footer .bphone {
  font-size: 30px;
  color: #1b1b1b;
}

.follow-buttons {
  float: right; width: 350px;
}

.follow-buttons a { 
  display: block;
  float: left;
  margin: 10px;
}

.footer .right-footer {
  margin-left: 348px;
}